{% extends 'layout.html.twig' %}
{% block title %} Items {% endblock %}


{% block body %}
    <section>
        <h1>Items</h1>
        <ul>
            {% for item in items %}
                <li><a href="item/{{ item.id }}" >{{ item.title }}</a></li>
            {% endfor %}
        </ul>
        <a href="/categories">Back to category</a>
    </section>
{% endblock %}

